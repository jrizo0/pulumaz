{
  "name": "@pulumaz/functions",
  "version": "0.0.0",
  "main": "dist/src/functions/httpTrigger.js",
  "scripts": {
    "build": "esbuild src/functions/*.ts --bundle --platform=node --target=node20 --outdir=dist/src/functions --external:@azure/functions",
    "watch": "esbuild src/functions/*.ts --bundle --platform=node --target=node20 --outdir=dist/src/functions --external:@azure/functions --watch",
    "clean": "rimraf dist",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "test": "echo \"No tests yet...\""
  },
  "dependencies": {
    "@pulumaz/core": "workspace:*",
    "hono": "^4.6.14"
  },
  "devDependencies": {
    "@azure/functions": "^4.6.0",
    "@types/node": "20.x",
    "azure-functions-core-tools": "^4.0.6610",
    "esbuild": "^0.20.0",
    "rimraf": "^5.0.0"
  }
}
